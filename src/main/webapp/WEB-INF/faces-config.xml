<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>        
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <navigation-handler>org.primefaces.mobile.application.MobileNavigationHandler</navigation-handler>
    </application>
    <managed-bean>
        <managed-bean-name>recaudacionGeneralController</managed-bean-name>
        <managed-bean-class>com.areatecnica.sigf.beans.recaudacion.RecaudacionGeneralController</managed-bean-class>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>recaudacionCompletaController</managed-bean-name>
        <managed-bean-class>com.areatecnica.sigf.beans.recaudacion.RecaudacionCompletaController</managed-bean-class>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>  
    <managed-bean>
        <managed-bean-name>recaudacionMinutoController</managed-bean-name>
        <managed-bean-class>com.areatecnica.sigf.beans.recaudacion.RecaudacionMinutoController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>recaudacionCombustibleController</managed-bean-name>
        <managed-bean-class>com.areatecnica.sigf.beans.recaudacion.RecaudacionCombustibleController</managed-bean-class>
