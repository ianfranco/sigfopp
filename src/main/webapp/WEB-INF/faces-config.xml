        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>recaudacionPieChartController</managed-bean-name>
        <managed-bean-class>com.areatecnica.sigf.charts.RecaudacionPieChartController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
